{
  "name": "nymphea-ui",
  "version": "0.0.0",
  "description": "sketch of the next js framework",
  "author": "https://github.com/Evawi",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "setupFiles": ["./test-env.js"],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.3.13",
    "css-loader": "^0.28.4",
    "enzyme": "^3.7.0",
    "extract-loader": "^3.0.0",
    "file-loader": "^0.11.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jquery": "^3.1.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.5.4",
    "style-loader": "^0.18.2",
    "underscore": "^1.9.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-auto-inject-version": "^1.0.0",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "dependencies": {
    "@material-ui/icons": "^3.0.1",
    "@material/button": "^0.40.1",
    "@material/ripple": "^0.40.1",
    "alertifyjs": "^1.11.0",
    "material-components-web": "^0.40.1",
    "semantic-ui-css": "^2.2.11"
  }
}
